# Rachel Environment Variables Example
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=ecto://USER:PASS@HOST/DATABASE

# Secret key base (generate with: mix phx.gen.secret)
SECRET_KEY_BASE=

# Phoenix
PHX_HOST=localhost
PORT=4000

# Admin Dashboard (defaults shown, change in production!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=rachel_admin_2024

# Sentry Error Tracking (optional)
# Get your DSN from https://sentry.io after creating a project
SENTRY_DSN=

# Production only
# PHX_SERVER=true
# ECTO_IPV6=true
# DNS_CLUSTER_QUERY=